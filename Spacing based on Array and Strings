const s = "Spacing"
const space = [1,3,5,7]
const array = s.split('')
console.log("arr",array,space[0])
let i=0

const data = array.map( data => {
    if(space[i] === 1 || space[i] === 5 || space[i] === 3 || space[i] === 7){
        i = i + 1
        return " " +data
    }else{
        return " " + data
    }
})

console.log("spacing values are:",data.join(''))
